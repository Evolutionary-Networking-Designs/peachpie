    <NoWarn>$(NoWarn);1591;1574</NoWarn>
    <PackageTags>peachpie;library;yaml</PackageTags>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Description>Peachpie PHP language library yaml functions.</Description>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="YamlDotNet" Version="9.1.4" />
